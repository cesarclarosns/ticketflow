    "passport-jwt": "^4.0.1",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.0.0",
    "@automock/jest": "^2.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@testcontainers/mongodb": "^10.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/mongoose-delete": "^1.0.3",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/react": "^18.2.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "jest": "^29.5.0",
    "node-mocks-http": "^1.13.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx",
      "jsx"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(ts|js|tsx|jsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(ts|js)"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      ".event.ts",
      ".constants.ts",
      ".config.ts",
      ".entity.ts",
      ".module.ts",
      ".index.ts",
      ".seeder.ts",
      ".strategy.ts",
      "<rootDir>/main.ts",
      "<rootDir>/seeder.ts",
      "<rootDir>/types.d.ts"
    ]
  }
}
